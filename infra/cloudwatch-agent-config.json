{
    "agent": {
      "metrics_collection_interval": 60,
      "run_as_user": "root"
    },
    "logs": {
      "logs_collected": {
        "files": {
          "collect_list": [
            {
              "file_path": "/var/log/syslog",
              "log_group_name": "syslog",
              "log_stream_name": "{instance_id}"
            },
            {
              "file_path": "/var/log/docker",
              "log_group_name": "docker",
              "log_stream_name": "{instance_id}"
            }
          ]
        }
      }
    }
  }